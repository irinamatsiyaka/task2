{
   "public": "dist",
   "rewrites": [{ "source": "**", "destination": "/index.html" }],
   "headers": [
      {
         "source": "**/*.js",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      },
      {
         "source": "**/*.css",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      },
      {
         "source": "**/*.png",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      },
      {
         "source": "**/*.jpg",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      }
   ]
}
